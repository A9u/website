- content_for :title do
  Testimonials | Josh Software Private Limited
.row
  .span12
    - @testimonies.each do |testimony|
      .well-trans.portfolio-site.testimonial{:id => testimony[:id]}
        .top
        .center.min-height
          .left
            %p.site-description
              "
              = testimony[:endorsement]
              "
            %ul.unstyled.geek-specs.pull-right
              %li
                %strong
                  = testimony[:endorser]
              %li
                = testimony[:title]
              %li
                = testimony[:company]
        .bottom
